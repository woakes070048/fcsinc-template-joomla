  <config>
    <fields name="params">
      <fieldset name="basic">
        <field name="logoFile" class="" type="media" default="" label="TPL_FCSINC_LOGO_LABEL" description="TPL_FCSINC_LOGO_DESC" />
        <field name="sitetitle" type="text" default="" label="JGLOBAL_TITLE" description="JFIELD_ALT_PAGE_TITLE_LABEL" filter="string" />
        <field name="sitedescription" type="text" default="" label="JGLOBAL_DESCRIPTION" description="JGLOBAL_SUBHEADING_DESC" filter="string" />
        <field name="html5" type="list" default="1" label="TPL_FCSINC_FIELD_HTML5_LABEL" description="TPL_FCSINC_FIELD_HTML5_DESC" filter="int">
          <option value="0">TPL_FCSINC_OPTION_XHTML</option>
          <option value="1">TPL_FCSINC_OPTION_HTML5</option>
        </field>
        <field name="content_frontpage" type="radio" default="1" label="TPL_FCSINC_FIELD_CONTENTFRONTPAGE_LABEL" description="TPL_FCSINC_FIELD_CONTENTFRONTPAGE_DESC" class="btn-group" filter="int">
          <option value="1">JSHOW</option>
          <option value="0">JHIDE</option>
        </field>
        <!--<field name="totop" type="list" default="1"				label="TPL_FCSINC_FIELD_TOTOP_LABEL"				description="TPL_FCSINC_FIELD_TOTOP_DESC"				filter="int">				<option value="1">JYES</option>				<option value="0">JNO</option>			</field>			-->
